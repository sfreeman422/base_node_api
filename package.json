{
  "name": "app",
  "version": "0.0.1",
  "description": "app",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "tsc -p tsconfig.prod.json",
    "format:check": "prettier --check 'src/**/*.ts'",
    "format:fix": "prettier --write 'src/**/*.ts'",
    "format:pre-commit": "pretty-quick --staged",
    "lint": "eslint '*/**/*.{js,ts}' --cache",
    "lint:fix": "eslint '*/**/*.{js,ts}' --cache --quiet --fix",
    "start": "npm run start:dev",
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.241.0",
    "body-parser": "^1.18.3",
    "dotenv": "^16.0.3",
    "express": "^4.16.4",
    "ioredis": "^5.2.4",
    "mysql": "^2.17.1",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-when": "^3.5.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  }
}
